<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="dot" format="reference" />
    <attr name="dotSpacing" format="dimension" />
    <attr name="dotCount" format="integer" />
    <attr name="align" format="enum">
        <enum name="left" value="0" />
        <enum name="center" value="1" />
        <enum name="right" value="2" />
    </attr>
    <declare-styleable name="DotIndicator">
        <attr name="dotCount" />
        <attr name="dotSpacing" />
        <attr name="dot" />
        <attr name="align" />
    </declare-styleable>
</resources>